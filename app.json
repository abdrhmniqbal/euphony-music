{
  "expo": {
    "newArchEnabled": true,
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "name": "emp",
    "slug": "emp",
    "splash": {
      "image": "./android/app/src/main/res/drawable-xxxhdpi/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./android/app/src/main/res/drawable-xxxhdpi/splashscreen_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#09090b"
      }
    },
    "extra": {
      "eas": {
        "projectId": "5db0649c-cba6-4a3a-b862-40d8e83f4e2c"
      }
    },
    "experiments": {
      "reactCompiler": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.euphony.music-player",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "dev.euphony.music_player",
      "permissions": [
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_AUDIO_PLAYBACK"
      ]
    },
    "plugins": ["expo-router", "expo-media-library"]
  }
}
